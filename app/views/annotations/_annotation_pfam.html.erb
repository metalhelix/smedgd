<div class="annotation_set" id="pfam">
  <h3><i class="icon-chevron-up"></i> Pfam Terms</h3>
  <table class="table annotation_table">
    <thead>
      <tr>
        <th>Term</th>
        <th>ID</th>
      </tr>
    </thead>
    <tbody>
      <% annotations.each do |annotation| %>
        <tr>
          <td><%= link_to_pfam(annotation.value) %></td>
          <td><%= annotation.value %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
